{
  "name": "moltydex-x402-agent",
  "version": "1.0.0",
  "description": "x402 Auto-Pay Agent for MoltyDEX - Automatically handles 402 Payment Required responses",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "example": "tsx src/examples/basic.ts",
    "example:wallet": "tsx src/examples/with-wallet.ts",
    "example:built": "node --loader ts-node/esm dist/examples/basic.js || node dist/examples/basic.js",
    "demo:video": "tsx src/examples/demo-video.ts"
  },
  "keywords": [
    "x402",
    "solana",
    "payment",
    "agent",
    "moltydex"
  ],
  "author": "MoltyDEX",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.3.11",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
